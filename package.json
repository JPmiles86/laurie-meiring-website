{
  "name": "laurie-meiring-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "build": "vite build",
    "vercel-build": "prisma generate && prisma db push && vite build",
    "preview": "vite preview",
    "server": "node server/server.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run server\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "migrate:data": "node scripts/migrate-data.js",
    "deploy:client": "bash scripts/deploy-client.sh",
    "deploy:dev": "bash scripts/deploy-dev.sh",
    "mode:client": "cp .env.client .env.local && echo '✅ Switched to CLIENT mode - AI features hidden'",
    "mode:dev": "cp .env.development .env.local && echo '✅ Switched to DEVELOPMENT mode - All features enabled'"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@googlemaps/markerclustererplus": "^1.2.10",
    "@prisma/client": "^6.10.1",
    "@tinymce/tinymce-react": "^5.1.1",
    "@vis.gl/react-google-maps": "^1.5.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "framer-motion": "^12.4.4",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.513.0",
    "multer": "^2.0.1",
    "node-fetch": "^3.3.2",
    "prisma": "^6.10.1",
    "prop-types": "^15.8.1",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^9.0.3",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.21.1",
    "rehype-raw": "^7.0.0",
    "sharp": "^0.34.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.0",
    "vite": "^5.0.8"
  }
}
